				</if>
				
					is_del,
				
		</trim>
		VALUES
		<trim prefix="(" suffix=")" suffixOverrides=",">
					
					<if test="id != null">
						#{id},
					</if>
					
					
					<if test="supplierNo != null">
						#{supplierNo},
					</if>
					
					
					<if test="oeCode != null">
