<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fjsaas.web.dao.SupplierOrmDao">
	<resultMap id="supplierOrm" type="SupplierOrm">
		<result property="id" column="id" />
		<result property="supplierNo" column="supplier_no" />
		<result property="oeCode" column="oe_code" />
		<result property="bigProductNo" column="big_product_no" />
		<result property="smallProductNo" column="small_product_no" />
		<result property="produerName" column="produer_name" />
		<result property="partName" column="part_name" />
		<result property="firstPrice" column="first_price" />
		<result property="secondPrice" column="second_price" />
		<result property="thirdPrice" column="third_price" />
		<result property="supplierId" column="supplier_id" />
		<result property="status" column="status" />
		<result property="createDate" column="create_date" />
		<result property="creatorId" column="creator_id" />
		<result property="updateDate" column="update_date" />
		<result property="updatorId" column="updator_id" />
		<result property="isDel" column="is_del" />
	</resultMap>
	<!-- Select SQL -->
	<sql id="supplierOrmSelector">
		SELECT
			<if test="fields != null">
			 	${fields}
			</if>
			<if test="fields == null">
				id,supplier_no,oe_code,big_product_no,small_product_no,produer_name,part_name,first_price,second_price,third_price,supplier_id,status,create_date,creator_id,update_date,updator_id,is_del	
			</if>
		FROM ass_supplier_orm
	</sql>
	
	<sql id="supplierOrmListWhere">
		<where>
				<if test="id != null">
					and id=#{id}
				</if>
				<if test="supplierNo != null">
					<if test="supplierNoLike == false">
						and supplier_no=#{supplierNo}
					</if>
					<if test="supplierNoLike == true">
						and  supplier_no like CONCAT("%",#{ supplierNo },"%")
					</if>
				</if>
				
				
				<if test="oeCode != null">
					<if test="oeCodeLike == false">
						and oe_code=#{oeCode}
					</if>
					<if test="oeCodeLike == true">
						and  oe_code like CONCAT("%",#{ oeCode },"%")
					</if>
				</if>
				
				
				<if test="bigProductNo != null">
					<if test="bigProductNoLike == false">
						and big_product_no=#{bigProductNo}
					</if>
					<if test="bigProductNoLike == true">
						and  big_product_no like CONCAT("%",#{ bigProductNo },"%")
					</if>
				</if>
				
				
				<if test="smallProductNo != null">
					<if test="smallProductNoLike == false">
						and small_product_no=#{smallProductNo}
					</if>
					<if test="smallProductNoLike == true">
						and  small_product_no like CONCAT("%",#{ smallProductNo },"%")
					</if>
				</if>
				
				
				<if test="produerName != null">
					<if test="produerNameLike == false">
						and produer_name=#{produerName}
					</if>
					<if test="produerNameLike == true">
						and  produer_name like CONCAT("%",#{ produerName },"%")
					</if>
				</if>
				
				
				<if test="partName != null">
					<if test="partNameLike == false">
						and part_name=#{partName}
					</if>
					<if test="partNameLike == true">
						and  part_name like CONCAT("%",#{ partName },"%")
					</if>
				</if>
				
				
				<if test="firstPrice != null">
					and first_price=#{firstPrice}
				</if>
				
				
				<if test="secondPrice != null">
					and second_price=#{secondPrice}
				</if>
				
				
				<if test="thirdPrice != null">
					and third_price=#{thirdPrice}
				</if>
				
				
				<if test="supplierId != null">
					and supplier_id=#{supplierId}
				</if>
				<if test="status != null">
					<if test="statusLike == false">
						and status=#{status}
					</if>
					<if test="statusLike == true">
						and  status like CONCAT("%",#{ status },"%")
					</if>
				</if>
				
				
				<if test="createDate != null">
					and create_date=#{createDate}
				</if>
				
				
				<if test="creatorId != null">
					and creator_id=#{creatorId}
				</if>
				<if test="updateDate != null">
					and update_date=#{updateDate}
				</if>
				
				
				<if test="updatorId != null">
					and updator_id=#{updatorId}
				</if>
				<if test="isDel != null">
					and is_del=#{isDel}
				</if>
				<if test="isDel == null">
					and is_del=0
				</if>
				
				
		</where>
	</sql>
	<sql id="supplierOrmListOrder">
		<if test="orderFields != null and orderFields.size >0">
		order by 
			<foreach collection="orderFields" separator="," item="orderField">
				${orderField.fieldName} ${orderField.order}
			</foreach>
		</if>
	</sql>

	<sql id="supplierOrmListLimit">
		<if test="startRow != null">
			limit #{startRow},#{pageSize}
		</if>
	</sql>

	<!-- 根据主键查询 -->
	<select id="getSupplierOrmByKey" resultMap="supplierOrm" parameterType="hashMap">
		SELECT id,supplier_no,oe_code,big_product_no,small_product_no,produer_name,part_name,first_price,second_price,third_price,supplier_id,status,create_date,creator_id,update_date,updator_id,is_del	 
		FROM ass_supplier_orm
		WHERE
		id=#{id}
		and is_del = 0
	</select>
	<select id="getSupplierOrmsByKeys" resultMap="supplierOrm" parameterType="ArrayList">
		SELECT id,supplier_no,oe_code,big_product_no,small_product_no,produer_name,part_name,first_price,second_price,third_price,supplier_id,status,create_date,creator_id,update_date,updator_id,is_del 
		FROM ass_supplier_orm
		WHERE
		id in
		<foreach collection="list" separator="," item="id" open="(" close=")">
			#{id}
		</foreach>
		and is_del = 0
	</select>

	<!-- 根据主键删除 -->
	<update id="deleteByKey" parameterType="hashMap">
		UPDATE ass_supplier_orm set is_del = 1 where id=#{id}
	</update>

	<!-- 根据主键批量删除 -->
	<update id="deleteByKeys">
		UPDATE ass_supplier_orm set is_del = 1 
		where id in
		<foreach collection="list" separator="," item="id" open="(" close=")">
			#{id}
		</foreach>
	</update>

	<!-- 根据主键更新 -->
	<update id="updateSupplierOrmByKey" parameterType="SupplierOrm">
		UPDATE ass_supplier_orm
		<set>
			<trim suffixOverrides=",">
				<if test="id != null">
					id = #{id},
				</if>
				<if test="supplierNo != null">
					supplier_no = #{supplierNo},
				</if>
				<if test="oeCode != null">
					oe_code = #{oeCode},
				</if>
				<if test="bigProductNo != null">
					big_product_no = #{bigProductNo},
				</if>
				<if test="smallProductNo != null">
					small_product_no = #{smallProductNo},
				</if>
				<if test="produerName != null">
					produer_name = #{produerName},
				</if>
				<if test="partName != null">
					part_name = #{partName},
				</if>
				<if test="firstPrice != null">
					first_price = #{firstPrice},
				</if>
				<if test="secondPrice != null">
					second_price = #{secondPrice},
				</if>
				<if test="thirdPrice != null">
					third_price = #{thirdPrice},
				</if>
				<if test="supplierId != null">
					supplier_id = #{supplierId},
				</if>
				<if test="status != null">
					status = #{status},
				</if>
				<if test="createDate != null">
					create_date = #{createDate},
				</if>
				<if test="creatorId != null">
					creator_id = #{creatorId},
				</if>
				<if test="updateDate != null">
					update_date = #{updateDate},
				</if>
				<if test="updatorId != null">
					updator_id = #{updatorId},
				</if>
				<if test="isDel != null">
					is_del = #{isDel},
				</if>
			</trim>
		</set>
		WHERE
		id=#{id}
	</update>
	<!-- 分页 -->
	<select id="getSupplierOrmListWithPage" parameterType="SupplierOrmQuery" resultMap="supplierOrm">
		<include refid="supplierOrmSelector" />
		<include refid="supplierOrmListWhere" />
		<include refid="supplierOrmListOrder" />
		<include refid="supplierOrmListLimit" />
	</select>
	<!-- 数据总数 -->
	<select id="getSupplierOrmListCount" parameterType="SupplierOrmQuery" resultType="int">
		SELECT count(1) FROM ass_supplier_orm
		<include refid="supplierOrmListWhere" />
	</select>
	<!-- 数据集合 -->
	<select id="getSupplierOrmList" parameterType="SupplierOrmQuery" resultMap="supplierOrm">
		<include refid="supplierOrmSelector" />
		<include refid="supplierOrmListWhere" />
		<include refid="supplierOrmListOrder" />
	</select>
	<!-- 添加 -->
	<insert id="addSupplierOrm" parameterType="SupplierOrm">
		INSERT INTO ass_supplier_orm
		<trim prefix="(" suffix=")" suffixOverrides=",">
				<if test="id != null">
					id,
				</if>
				
				<if test="supplierNo != null">
					supplier_no,
				</if>
				
				<if test="oeCode != null">
					oe_code,
				</if>
				
				<if test="bigProductNo != null">
					big_product_no,
				</if>
				
				<if test="smallProductNo != null">
					small_product_no,
				</if>
				
				<if test="produerName != null">
					produer_name,
				</if>
				
				<if test="partName != null">
					part_name,
				</if>
				
				<if test="firstPrice != null">
					first_price,
				</if>
				
				<if test="secondPrice != null">
					second_price,
				</if>
				
				<if test="thirdPrice != null">
					third_price,
				</if>
				
				<if test="supplierId != null">
					supplier_id,
				</if>
				
				<if test="status != null">
					status,
				</if>
				
				<if test="createDate != null">
