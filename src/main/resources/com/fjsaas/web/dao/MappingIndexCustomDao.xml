<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fjsaas.web.dao.MappingIndexCustomDao">
	<resultMap id="mappingIndex" type="MappingIndex">
		<result property="id" column="id" />
		<result property="productBrand" column="product_brand" />
		<result property="productName" column="product_name" />
		<result property="productCode" column="product_code" />
		<result property="productSize" column="product_size" />
		<result property="referenceCode" column="reference_code" />
		<result property="referenceBrand" column="reference_brand" />
		<result property="firstPrice" column="first_price" />
		<result property="secondPrice" column="second_price" />
		<result property="thirdPrice" column="third_price" />
		<result property="oeCode" column="oe_code" />
		<result property="autoTypeId" column="auto_type_id" />
		<result property="partSortId" column="part_sort_id" />
		<result property="status" column="status" />
		<result property="supplierId" column="supplier_id" />
		<result property="mappingId" column="mapping_id" />
		<result property="createDate" column="create_date" />
		<result property="creatorId" column="creator_id" />
		<result property="updateDate" column="update_date" />
		<result property="updatorId" column="updator_id" />
		<result property="mappingCreateDate" column="mapping_create_date" />
		<result property="mappingUpdateDate" column="mapping_update_date" />
		<result property="isDel" column="is_del" />
		<result property="imageUrl" column="image_url" />
	</resultMap>
	<!-- Select SQL -->
	<sql id="mappingIndexSelector">
		SELECT
			<if test="fields != null">
			 	${fields}
			</if>
			<if test="fields == null">
				id,product_brand,product_name,product_code,product_size,reference_code,reference_brand,first_price,second_price,third_price,oe_code,auto_type_id,part_sort_id,status,supplier_id,mapping_id,create_date,creator_id,update_date,updator_id,mapping_create_date,mapping_update_date,is_del,image_url	
			</if>
		FROM ass_mapping_index
	</sql>
	<sql id="mappingIndexDistinctSelector">
		SELECT DISTINCT 
			product_brand,product_name,product_code,product_size,first_price,second_price,third_price,part_sort_id,status,supplier_id,mapping_id,is_del,image_url	 
		FROM
	</sql>
		
	<sql id="mappingIndexListWhere">
		<where>
				<if test="id != null">
					and id=#{id}
				</if>
				<if test="productBrand != null">
					<if test="productBrandLike == false">
						and product_brand=#{productBrand}
					</if>
					<if test="productBrandLike == true">
						and  product_brand like CONCAT("%",#{ productBrand },"%")
					</if>
				</if>
				<if test="productName != null">
					<if test="productNameLike == false">
						and product_name=#{productName}
					</if>
					<if test="productNameLike == true">
						and  product_name like CONCAT("%",#{ productName },"%")
					</if>
				</if>
				<if test="productCode != null">
					<if test="productCodeLike == false">
						and product_code=#{productCode}
					</if>
					<if test="productCodeLike == true">
						and  product_code like CONCAT("%",#{ productCode },"%")
					</if>
				</if>
				<if test="productSize != null">
					<if test="productSizeLike == false">
						and product_size=#{productSize}
					</if>
					<if test="productSizeLike == true">
						and  product_size like CONCAT("%",#{ productSize },"%")
					</if>
				</if>
				<if test="referenceCode != null">
					<if test="referenceCodeLike == false">
						and reference_code=#{referenceCode}
					</if>
					<if test="referenceCodeLike == true">
						and  reference_code like CONCAT("%",#{ referenceCode },"%")
					</if>
				</if>
				<if test="referenceBrand != null">
					<if test="referenceBrandLike == false">
						and reference_brand=#{referenceBrand}
					</if>
					<if test="referenceBrandLike == true">
						and  reference_brand like CONCAT("%",#{ referenceBrand },"%")
					</if>
				</if>
				<if test="firstPrice != null">
					and first_price=#{firstPrice}
				</if>
				<if test="secondPrice != null">
					and second_price=#{secondPrice}
				</if>
				<if test="thirdPrice != null">
					and third_price=#{thirdPrice}
				</if>
				<if test="oeCode != null">
					<if test="oeCodeLike == false">
						and oe_code=#{oeCode}
					</if>
					<if test="oeCodeLike == true">
						and  oe_code like CONCAT("%",#{ oeCode },"%")
					</if>
				</if>
				<if test="autoTypeId != null">
					and auto_type_id=#{autoTypeId}
				</if>
				<if test="partSortId != null">
					and part_sort_id=#{partSortId}
				</if>
				<if test="status != null">
					<if test="statusLike == false">
						and status=#{status}
					</if>
					<if test="statusLike == true">
						and  status like CONCAT("%",#{ status },"%")
					</if>
				</if>
				<if test="supplierId != null">
					and supplier_id=#{supplierId}
				</if>
				<if test="mappingId != null">
					and mapping_id=#{mappingId}
				</if>
				<if test="createDate != null">
					and create_date=#{createDate}
				</if>
				<if test="creatorId != null">
					and creator_id=#{creatorId}
				</if>
				<if test="updateDate != null">
					and update_date=#{updateDate}
				</if>
				<if test="updatorId != null">
					and updator_id=#{updatorId}
				</if>
				<if test="mappingCreateDate != null">
					and mapping_create_date=#{mappingCreateDate}
				</if>
				<if test="mappingUpdateDate != null">
					and mapping_update_date=#{mappingUpdateDate}
				</if>
				<if test="isDel != null">
					and is_del=#{isDel}
				</if>
				<if test="isDel == null">
					and is_del=0
				</if>
				<if test="imageUrl != null">
					<if test="imageUrlLike == false">
						and image_url=#{imageUrl}
					</if>
					<if test="imageUrlLike == true">
						and  image_url like CONCAT("%",#{ imageUrl },"%")
					</if>
				</if>
		</where>
	</sql>
	<sql id="mappingIndexListOrder">
		<if test="orderFields != null and orderFields.size >0">
		order by 
			<foreach collection="orderFields" separator="," item="orderField">
				${orderField.fieldName} ${orderField.order}
			</foreach>
		</if>
	</sql>

	<sql id="mappingIndexListLimit">
		<if test="startRow != null">
			limit #{startRow},#{pageSize}
		</if>
	</sql>

	<!-- List -->
	<select id="getMappingIndexListByMulCondition" parameterType="MappingIndexQuery" resultMap="mappingIndex">
		<include refid="mappingIndexDistinctSelector" />
		(
		<include refid="mappingIndexSelector" />
		<where>
			<trim prefix="(" suffix=")" prefixOverrides="or">
				<if test="productName!=null">
					or product_brand = #{productName}
				</if>
				<if test="productName!=null">
					or product_name = #{productName}
				</if>
				<if test="productCode!=null">
					or product_code = #{productCode}
				</if>
			</trim>
			<if test="autoTypeId!=null">
				and auto_type_id=#{autoTypeId}
			</if>
			<if test="partSortId!=null">
				and part_sort_id=#{partSortId}
			</if>
			<if test="productBrand!=null">
				and product_brand=#{productBrand}
			</if>
			<if test="isDel != null">
				and is_del=#{isDel}
			</if>
			<if test="isDel == null">
				and is_del=0
			</if>	
		</where>
		) as t
		<include refid="mappingIndexListOrder" />
	</select>

	<!-- 分页 -->
	<select id="getMappingIndexListWithPageByMulCondition" parameterType="MappingIndexQuery" resultMap="mappingIndex">
		<include refid="mappingIndexDistinctSelector" />
		(
		<include refid="mappingIndexSelector" />
		<where>
			<trim prefix="(" suffix=")" prefixOverrides="or">
				<if test="productName!=null">
					or product_brand = #{productName}
				</if>
				<if test="productName!=null">
					or product_name = #{productName}
				</if>
				<if test="productCode!=null">
					or product_code = #{productCode}
				</if>
			</trim>
			<if test="autoTypeId!=null">
				and auto_type_id=#{autoTypeId}
			</if>
			<if test="partSortId!=null">
				and part_sort_id=#{partSortId}
			</if>
			<if test="productBrand!=null">
				and product_brand=#{productBrand}
			</if>
			<if test="isDel != null">
				and is_del=#{isDel}
			</if>
			<if test="isDel == null">
				and is_del=0
			</if>	
		</where>
		) as t
		<include refid="mappingIndexListOrder" />
		<include refid="mappingIndexListLimit" />
	</select>
	<!-- 数据总数 -->
	<select id="getMappingIndexListCountByMulCondition" parameterType="MappingIndexQuery" resultType="int">
		SELECT count(1) FROM 
		(
			<include refid="mappingIndexDistinctSelector" />
			(
			<include refid="mappingIndexSelector" />
			<where>
				<trim prefix="(" suffix=")" prefixOverrides="or">
					<if test="productName!=null">
						or product_brand = #{productName}
					</if>
					<if test="productName!=null">
						or product_name = #{productName}
					</if>
					<if test="productCode!=null">
						or product_code = #{productCode}
					</if>
				</trim>
				<if test="autoTypeId!=null">
					and auto_type_id=#{autoTypeId}
				</if>
				<if test="partSortId!=null">
					and part_sort_id=#{partSortId}
				</if>
				<if test="productBrand!=null">
					and product_brand=#{productBrand}
				</if>
				<if test="isDel != null">
					and is_del=#{isDel}
				</if>
				<if test="isDel == null">
					and is_del=0
				</if>	
			</where>
			) as t
		) as t2
	</select>
	
	<!-- 根据MappingId更新 -->
	<update id="updateMappingIndexByMappingId" parameterType="MappingIndex">
		UPDATE ass_mapping_index
		<set>
			<trim suffixOverrides=",">
				<if test="id != null">
					id = #{id},
				</if>
				<if test="productBrand != null">
					product_brand = #{productBrand},
				</if>
				<if test="productName != null">
					product_name = #{productName},
				</if>
				<if test="productCode != null">
					product_code = #{productCode},
				</if>
				<if test="productSize != null">
					product_size = #{productSize},
				</if>
				<if test="referenceCode != null">
					reference_code = #{referenceCode},
				</if>
				<if test="referenceBrand != null">
					reference_brand = #{referenceBrand},
				</if>
				<if test="firstPrice != null">
					first_price = #{firstPrice},
				</if>
				<if test="secondPrice != null">
					second_price = #{secondPrice},
				</if>
				<if test="thirdPrice != null">
					third_price = #{thirdPrice},
				</if>
				<if test="oeCode != null">
					oe_code = #{oeCode},
				</if>
				<if test="autoTypeId != null">
					auto_type_id = #{autoTypeId},
				</if>
				<if test="partSortId != null">
					part_sort_id = #{partSortId},
				</if>
				<if test="status != null">
					status = #{status},
				</if>
				<if test="supplierId != null">
					supplier_id = #{supplierId},
				</if>
				<if test="mappingId != null">
					mapping_id = #{mappingId},
				</if>
				<if test="createDate != null">
					create_date = #{createDate},
				</if>
				<if test="creatorId != null">
					creator_id = #{creatorId},
				</if>
				<if test="updateDate != null">
					update_date = #{updateDate},
				</if>
				<if test="updatorId != null">
					updator_id = #{updatorId},
				</if>
				<if test="mappingCreateDate != null">
					mapping_create_date = #{mappingCreateDate},
				</if>
				<if test="mappingUpdateDate != null">
					mapping_update_date = #{mappingUpdateDate},
				</if>
				<if test="isDel != null">
					is_del = #{isDel},
				</if>
				<if test="imageUrl != null">
					image_url = #{imageUrl},
				</if>
			</trim>
		</set>
		WHERE
		mapping_id=#{mappingId}
	</update>

</mapper>